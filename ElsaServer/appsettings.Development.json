{
  "Email": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "DefaultSender": "YOUR_EMAIL",
    "Password": "YOUR_GMAIL_APP_PASSWORD",
    "UserName": "YOUR_EMAIL",
    "RequireCredentials": true,
    "SecureSocketOptions": "StartTlsWhenAvailable"
  },
  "Webhooks": {
    "Endpoints": [
      {
        "EventTypes": ["RunTask"],
        "Url": "http://localhost:5002/api/webhooks/run-task"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
